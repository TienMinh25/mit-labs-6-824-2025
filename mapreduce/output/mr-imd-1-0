ive","v":"1"},{"k":"in","v":"1"},{"k":"her","v":"1"},{"k":"I","v":"1"},{"k":"pulled","v":"1"},{"k":"all","v":"1"},{"k":"around","v":"1"},{"k":"her","v":"1"},{"k":"and","v":"1"},{"k":"hollered","v":"1"},{"k":"a","v":"1"},{"k":"little","v":"1"},{"k":"but","v":"1"},{"k":"there","v":"1"},{"k":"wasn","v":"1"},{"k":"t","v":"1"},{"k":"any","v":"1"},{"k":"answer","v":"1"},{"k":"all","v":"1"},{"k":"dead","v":"1"},{"k":"still","v":"1"},{"k":"I","v":"1"},{"k":"felt","v":"1"},{"k":"a","v":"1"},{"k":"little","v":"1"},{"k":"bit","v":"1"},{"k":"heavy","v":"1"},{"k":"hearted","v":"1"},{"k":"about","v":"1"},{"k":"the","v":"1"},{"k":"gang","v":"1"},{"k":"but","v":"1"},{"k":"not","v":"1"},{"k":"much","v":"1"},{"k":"for","v":"1"},{"k":"I","v":"1"},{"k":"reckoned","v":"1"},{"k":"if","v":"1"},{"k":"they","v":"1"},{"k":"could","v":"1"},{"k":"stand","v":"1"},{"k":"it","v":"1"},{"k":"I","v":"1"},{"k":"could","v":"1"},{"k":"Then","v":"1"},{"k":"here","v":"1"},{"k":"comes","v":"1"},{"k":"the","v":"1"},{"k":"ferryboat","v":"1"},{"k":"so","v":"1"},{"k":"I","v":"1"},{"k":"shoved","v":"1"},{"k":"for","v":"1"},{"k":"the","v":"1"},{"k":"middle","v":"1"},{"k":"of","v":"1"},{"k":"the","v":"1"},{"k":"river","v":"1"},{"k":"on","v":"1"},{"k":"a","v":"1"},{"k":"long","v":"1"},{"k":"down","v":"1"},{"k":"stream","v":"1"},{"k":"slant","v":"1"},{"k":"and","v":"1"},{"k":"when","v":"1"},{"k":"I","v":"1"},{"k":"judged","v":"1"},{"k":"I","v":"1"},{"k":"was","v":"1"},{"k":"out","v":"1"},{"k":"of","v":"1"},{"k":"eye","v":"1"},{"k":"reach","v":"1"},{"k":"I","v":"1"},{"k":"laid","v":"1"},{"k":"on","v":"1"},{"k":"my","v":"1"},{"k":"oars","v":"1"},{"k":"and","v":"1"},{"k":"looked","v":"1"},{"k":"back","v":"1"},{"k":"and","v":"1"},{"k":"see","v":"1"},{"k":"her","v":"1"},{"k":"go","v":"1"},{"k":"and","v":"1"},{"k":"smell","v":"1"},{"k":"around","v":"1"},{"k":"the","v":"1"},{"k":"wreck","v":"1"},{"k":"for","v":"1"},{"k":"Miss","v":"1"},{"k":"Hooker","v":"1"},{"k":"s","v":"1"},{"k":"remainders","v":"1"},{"k":"because","v"
ast","v":"1"},{"k":"time","v":"1"},{"k":"most","v":"1"},{"k":"busted","v":"1"},{"k":"his","v":"1"},{"k":"brains","v":"1"},{"k":"out","v":"1"},{"k":"he","v":"1"},{"k":"thought","v":"1"},{"k":"he","v":"1"},{"k":"d","v":"1"},{"k":"got","v":"1"},{"k":"to","v":"1"},{"k":"give","v":"1"},{"k":"it","v":"1"},{"k":"up","v":"1"},{"k":"but","v":"1"},{"k":"after","v":"1"},{"k":"he","v":"1"},{"k":"was","v":"1"},{"k":"rested","v":"1"},{"k":"he","v":"1"},{"k":"allowed","v":"1"},{"k":"he","v":"1"},{"k":"would","v":"1"},{"k":"give","v":"1"},{"k":"her","v":"1"},{"k":"one","v":"1"},{"k":"more","v":"1"},{"k":"turn","v":"1"},{"k":"for","v":"1"},{"k":"luck","v":"1"},{"k":"and","v":"1"},{"k":"this","v":"1"},{"k":"time","v":"1"},{"k":"he","v":"1"},{"k":"made","v":"1"},{"k":"the","v":"1"},{"k":"trip","v":"1"},{"k":"In","v":"1"},{"k":"the","v":"1"},{"k":"morning","v":"1"},{"k":"we","v":"1"},{"k":"was","v":"1"},{"k":"up","v":"1"},{"k":"at","v":"1"},{"k":"break","v":"1"},{"k":"of","v":"1"},{"k":"day","v":"1"},{"k":"and","v":"1"},{"k":"down","v":"1"},{"k":"to","v":"1"},{"k":"the","v":"1"},{"k":"nigger","v":"1"},{"k":"cabins","v":"1"},{"k":"to","v":"1"},{"k":"pet","v":"1"},{"k":"the","v":"1"},{"k":"dogs","v":"1"},{"k":"and","v":"1"},{"k":"make","v":"1"},{"k":"friends","v":"1"},{"k":"with","v":"1"},{"k":"the","v":"1"},{"k":"nigger","v":"1"},{"k":"that","v":"1"},{"k":"fed","v":"1"},{"k":"Jim","v":"1"},{"k":"if","v":"1"},{"k":"it","v":"1"},{"k":"was","v":"1"},{"k":"Jim","v":"1"},{"k":"that","v":"1"},{"k":"was","v":"1"},{"k":"being","v":"1"},{"k":"fed","v":"1"},{"k":"The","v":"1"},{"k":"niggers","v":"1"},{"k":"was","v":"1"},{"k":"just","v":"1"},{"k":"getting","v":"1"},{"k":"through","v":"1"},{"k":"breakfast","v":"1"},{"k":"and","v":"1"},{"k":"starting","v":"1"},{"k":"for","v":"1"},{"k":"the","v":"1"},{"k":"fields","v":"1"},{"k":"and","v":"1"},{"k":"Jim","v":"1"},{"k":"s","v":"1"},{"k":"nigger","v":"1"},{"k":"was","v":"1"},{"k":"piling","v":"1"},{"k":"up","v":"1"},{"k":"a","v":"1"},{"k":"tin","v":"1"}
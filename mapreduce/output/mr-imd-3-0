ver","v":"1"},{"k":"has","v":"1"},{"k":"been","v":"1"},{"k":"any","v":"1"},{"k":"such","v":"1"},{"k":"person","v":"1"},{"k":"Lestrade","v":"1"},{"k":"looked","v":"1"},{"k":"sadly","v":"1"},{"k":"at","v":"1"},{"k":"my","v":"1"},{"k":"companion","v":"1"},{"k":"Then","v":"1"},{"k":"he","v":"1"},{"k":"turned","v":"1"},{"k":"to","v":"1"},{"k":"me","v":"1"},{"k":"tapped","v":"1"},{"k":"his","v":"1"},{"k":"forehead","v":"1"},{"k":"three","v":"1"},{"k":"times","v":"1"},{"k":"shook","v":"1"},{"k":"his","v":"1"},{"k":"head","v":"1"},{"k":"solemnly","v":"1"},{"k":"and","v":"1"},{"k":"hurried","v":"1"},{"k":"away","v":"1"},{"k":"He","v":"1"},{"k":"had","v":"1"},{"k":"hardly","v":"1"},{"k":"shut","v":"1"},{"k":"the","v":"1"},{"k":"door","v":"1"},{"k":"behind","v":"1"},{"k":"him","v":"1"},{"k":"when","v":"1"},{"k":"Holmes","v":"1"},{"k":"rose","v":"1"},{"k":"to","v":"1"},{"k":"put","v":"1"},{"k":"on","v":"1"},{"k":"his","v":"1"},{"k":"overcoat","v":"1"},{"k":"There","v":"1"},{"k":"is","v":"1"},{"k":"something","v":"1"},{"k":"in","v":"1"},{"k":"what","v":"1"},{"k":"the","v":"1"},{"k":"fellow","v":"1"},{"k":"says","v":"1"},{"k":"about","v":"1"},{"k":"outdoor","v":"1"},{"k":"work","v":"1"},{"k":"he","v":"1"},{"k":"remarked","v":"1"},{"k":"so","v":"1"},{"k":"I","v":"1"},{"k":"think","v":"1"},{"k":"Watson","v":"1"},{"k":"that","v":"1"},{"k":"I","v":"1"},{"k":"must","v":"1"},{"k":"leave","v":"1"},{"k":"you","v":"1"},{"k":"to","v":"1"},{"k":"your","v":"1"},{"k":"papers","v":"1"},{"k":"for","v":"1"},{"k":"a","v":"1"},{"k":"little","v":"1"},{"k":"It","v":"1"},{"k":"was","v":"1"},{"k":"after","v":"1"},{"k":"five","v":"1"},{"k":"o","v":"1"},{"k":"clock","v":"1"},{"k":"when","v":"1"},{"k":"Sherlock","v":"1"},{"k":"Holmes","v":"1"},{"k":"left","v":"1"},{"k":"me","v":"1"},{"k":"but","v":"1"},{"k":"I","v":"1"},{"k":"had","v":"1"},{"k":"no","v":"1"},{"k":"time","v":"1"},{"k":"to","v":"1"},{"k":"be","v":"1"},{"k":"lonely","v":"1"},{"k":"for","v":"1"},{"k":"within","v":"1"},{"k":"an","v":"1"},{"k"
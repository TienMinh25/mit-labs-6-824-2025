ions","v":"1"},{"k":"that","v":"1"},{"k":"prevail","v":"1"},{"k":"in","v":"1"},{"k":"this","v":"1"},{"k":"flat","v":"1"},{"k":"and","v":"1"},{"k":"with","v":"1"},{"k":"this","v":"1"},{"k":"family","v":"1"},{"k":"here","v":"1"},{"k":"he","v":"1"},{"k":"looked","v":"1"},{"k":"briefly","v":"1"},{"k":"but","v":"1"},{"k":"decisively","v":"1"},{"k":"at","v":"1"},{"k":"the","v":"1"},{"k":"floor","v":"1"},{"k":"I","v":"1"},{"k":"give","v":"1"},{"k":"immediate","v":"1"},{"k":"notice","v":"1"},{"k":"on","v":"1"},{"k":"my","v":"1"},{"k":"room","v":"1"},{"k":"For","v":"1"},{"k":"the","v":"1"},{"k":"days","v":"1"},{"k":"that","v":"1"},{"k":"I","v":"1"},{"k":"have","v":"1"},{"k":"been","v":"1"},{"k":"living","v":"1"},{"k":"here","v":"1"},{"k":"I","v":"1"},{"k":"will","v":"1"},{"k":"of","v":"1"},{"k":"course","v":"1"},{"k":"pay","v":"1"},{"k":"nothing","v":"1"},{"k":"at","v":"1"},{"k":"all","v":"1"},{"k":"on","v":"1"},{"k":"the","v":"1"},{"k":"contrary","v":"1"},{"k":"I","v":"1"},{"k":"will","v":"1"},{"k":"consider","v":"1"},{"k":"whether","v":"1"},{"k":"to","v":"1"},{"k":"proceed","v":"1"},{"k":"with","v":"1"},{"k":"some","v":"1"},{"k":"kind","v":"1"},{"k":"of","v":"1"},{"k":"action","v":"1"},{"k":"for","v":"1"},{"k":"damages","v":"1"},{"k":"from","v":"1"},{"k":"you","v":"1"},{"k":"and","v":"1"},{"k":"believe","v":"1"},{"k":"me","v":"1"},{"k":"it","v":"1"},{"k":"would","v":"1"},{"k":"be","v":"1"},{"k":"very","v":"1"},{"k":"easy","v":"1"},{"k":"to","v":"1"},{"k":"set","v":"1"},{"k":"out","v":"1"},{"k":"the","v":"1"},{"k":"grounds","v":"1"},{"k":"for","v":"1"},{"k":"such","v":"1"},{"k":"an","v":"1"},{"k":"action","v":"1"},{"k":"He","v":"1"},{"k":"was","v":"1"},{"k":"silent","v":"1"},{"k":"and","v":"1"},{"k":"looked","v":"1"},{"k":"straight","v":"1"},{"k":"ahead","v":"1"},{"k":"as","v":"1"},{"k":"if","v":"1"},{"k":"waiting","v":"1"},{"k":"for","v":"1"},{"k":"something","v":"1"},{"k":"And","v":"1"},{"k":"indeed","v":"1"},{"k":"his","v":"1"},{"k":"two","v":"1"},{"k":"friends","v":"1"}